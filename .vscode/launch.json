{
    "configurations": [
        {
            "name": "(gdb) Launch",
            "type": "cppdbg",
            "request": "launch",
            // "program": "${workspaceFolder}/bazel-bin/my_servable/hashmap_server_example",
            "program": "${workspaceFolder}/bazel-bin/my_servable/hashmap_manager_example",
            // "args": [
            //     "--tb-to-dut",
            //     "/tmp/test_tb_to_dut",
            //     "--dut-to-tb",
            //     "/tmp/test_dut_to_tb"
            // ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}